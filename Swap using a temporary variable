#include <stdio.h>

int main() {
    int a, b, temp;

    printf("Enter two integers (a and b): ");
    scanf("%d %d", &a, &b);

    printf("Before swap: a = %d, b = %d\n", a, b);

    // Swap using a temporary variable
    temp = a;
    a = b;
    b = temp;

    printf("After swap:  a = %d, b = %d\n", a, b);

    return 0;
}
